{
  "name": "esquire-frontend",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "description": "Esquire Frontend",
  "scripts": {
    "start": "node scripts/start.js",
    "prebuild": "npm run clean",
    "build": "node scripts/build.js",
    "serve": "http-server ./dist -p 5000",
